<h3><%= @search.condition_name %> ページの編集</h3>

<%= form_with(model: @search) do |f| %>
    <%= f.label :condition_name, '症状名' %>
    <%= f.text_field :condition_name %>
    <%= f.label :content, '不足している栄養素' %>
    <%= f.text_field :content %>
    
    <%= f.submit '投稿' %>
<% end %>

<%= link_to '戻る', search_path %>